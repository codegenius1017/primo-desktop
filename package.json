{
  "name": "primo",
  "productName": "primo",
  "version": "1.0.0",
  "description": "Svelte + Electron Forge",
  "main": "src/index.cjs",
  "type": "module",
  "scripts": {
    "electron": "electron .",
    "start": "concurrently \"npm:dev\" \"electron .\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "svelte-build": "rollup -c",
    "svelte-dev": "rollup -c -w",
    "svelte-start": "sirv public",
    "dev-vite": "npm link @primo-app/primo && vite --force",
    "serve": "vite preview",
    "dev": "npm link @primo-app/primo && svelte-kit dev -p 3333",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "keywords": [],
  "author": "mateomorris",
  "license": "AGPL",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "crawler"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.4",
    "@primo-app/primo": "file:../primo",
    "autoprefixer": "^10.3.4",
    "axios": "^0.21.4",
    "electron": "^14.0.0",
    "electron-reload": "^1.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "esm": "^3.2.25",
    "file-saver": "^2.0.5",
    "fs-extra": "^10.0.0",
    "idb-keyval": "^6.0.2",
    "install": "^0.13.0",
    "lodash-es": "^4.17.21",
    "postcss-nested": "^5.0.6",
    "promise-worker": "^2.0.1",
    "sirv-cli": "^0.4.4",
    "svelte-preprocess": "^4.9.1",
    "tailwindcss": "^2.2.14"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.50",
    "@electron-forge/maker-deb": "^6.0.0-beta.50",
    "@electron-forge/maker-rpm": "^6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
    "@electron-forge/maker-zip": "^6.0.0-beta.50",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@sveltejs/kit": "next",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.11",
    "@tsconfig/svelte": "^2.0.1",
    "concurrently": "^5.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rollup": "^1.32.1",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.34.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "vite": "^2.5.4"
  }
}
