{
  "name": "primo",
  "version": "1.0.0",
  "main": "./src/main.ts",
  "scripts": {
    "package": "npx electron-packager . --overwrite --icon='./primo.icns'",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "electron": "run-s build pure-electron",
    "electron-dev": "run-p dev pure-electron-dev",
    "dist-darwin": "run-s build pure-dist-darwin",
    "pure-electron": "electron .",
    "pure-electron-dev": "cross-env NODE_ENV=development electron .",
    "pure-dist-darwin": "electron-builder --dir --mac --config electron-builder.yml",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^3.1.0",
    "@tsconfig/svelte": "^1.0.0",
    "chokidar": "^3.4.1",
    "cross-env": "^7.0.2",
    "electron": "^6.1.12",
    "electron-builder": "^21.2.0",
    "electron-packager": "^15.0.0",
    "npm-run-all": "^4.1.5",
    "precss": "^4.0.0",
    "primo-builder": "^1.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^5.3.0",
    "sirv-cli": "^0.4.4",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "body-parser": "^1.19.0",
    "bulma": "^0.9.0",
    "clean-css": "^4.2.3",
    "electron-dl": "^3.0.1",
    "fs": "0.0.1-security",
    "jszip": "^3.5.0",
    "lowdb": "^1.0.0",
    "node-sass": "^4.14.1",
    "node-zip": "^1.1.1",
    "path": "^0.12.7",
    "polka": "^0.5.2",
    "postcss": "^7.0.32",
    "postcss-nested": "^4.2.3",
    "primo-app": "^1.0.53",
    "primo-functions": "^1.0.13",
    "serve-static": "^1.14.1",
    "sirv-cli": "^1.0.0",
    "tailwindcss": "^1.6.0"
  }
}
